textformatting = {
	format = {
		name = console_success
		override = no
		format = "color:{1,1,1}"
	}

	format = {
		name = console_failure
		override = no
		format = "color:{1,1,0}"
	}

    format = {
        name = console_autocomplete
        override = no
        format = "color:{.7,.7,.7}"
    }

	format = {
        name = console_command
        override = no
        format = "color:{1,1,1}"
    }
}
